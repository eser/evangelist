{
    "extends": [
        "eser"
    ],
    "env": {
        "node": true,
        "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "settings": {
        "import/parsers": {
            "@typescript-eslint/parser": [ ".ts", ".tsx" ]
        },
        "import/resolver": {
            "typescript": {}
        }
    }
}
